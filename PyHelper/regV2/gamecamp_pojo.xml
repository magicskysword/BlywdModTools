<?xml version="1.0" encoding="utf-8"?>

<bean name="gamecamp_pojo">
	<variable name="camp_id" fromCol="阵营ID" type="string" />
	<variable name="display_name" fromCol="阵营名称" type="string" />
	<variable name="camp_type" fromCol="阵营类别" type="int" defaultValue="0" />
	<variable name="camp_leader" fromCol="阵营领袖" type="string" />
	<variable name="leader_called" fromCol="领袖称呼" type="string" />
	<variable name="camp_logo" fromCol="势力徽记" type="string" />
	<variable name="aptitude_to_player" fromCol="与玩家关系" type="string" />
	<variable name="init_balance_val" fromCol="初始平衡值" type="int" defaultValue="0" />
	<variable name="init_resources" fromCol="初始资源" type="string" />
	<variable name="init_stat" fromCol="初始属性" type="string" />
	<variable name="territory" fromCol="初始领地" type="string" />
