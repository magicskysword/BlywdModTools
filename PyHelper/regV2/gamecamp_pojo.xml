	<variable name="init_active_state" fromCol="初始活跃状态" type="int" defaultValue="0" />
	<variable name="init_camp_rl_weight" fromCol="初始阵营关系权值" type="int" defaultValue="0" />
	<variable name="is_hidden" fromCol="是否隐藏" type="int" defaultValue="0" />
	<variable name="tag" fromCol="标记" type="string"/>
	<variable name="desc" fromCol="阵营简介" type="string"/>
